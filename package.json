{
  "name": "@agentlab/ldkg-ui-react",
  "author": "Alexey Ivanov <amivanoff@gmail.com>",
  "version": "0.1.0",
  "license": "EPL-2.0",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint \"{src,stories,test}**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "./node_modules/.bin/prettier --write \"{src,stories,test}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint-staged": "lint-staged"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest": "^26.0.23",
    "@types/json-pointer": "^1.0.30",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.3",
    "lint-staged": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "ts-jest": "^26.5.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "**/@types/jest": "^26.0.23",
    "**/@typescript-eslint/eslint-plugin": "^4.22.1",
    "**/@typescript-eslint/parser": "^4.22.1",
    "**/eslint": "^7.26.0",
    "**/eslint-config-prettier": "^8.3.0",
    "**/eslint-config-react-app": "^6.0.0",
    "**/eslint-plugin-import": "^2.22.1",
    "**/eslint-plugin-jsx-a11y": "^6.4.1",
    "**/eslint-plugin-prettier": "^3.4.0",
    "**/eslint-plugin-react": "^7.23.2",
    "**/eslint-plugin-react-hooks": "^4.2.0",
    "**/jest": "^26.6.3",
    "**/jest-watch-typeahead": "^0.6.3",
    "**/ts-jest": "^26.5.6",
    "**/tslib": "^2.2.0",
    "**/typescript": "^4.2.4"
  },
  "lint-staged": {
    "{src,stories,test}/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "{src,stories,test}/**/*.{css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "module": "dist/ldkg-ui-react.esm.js",
  "size-limit": [
    {
      "path": "dist/ldkg-ui-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ldkg-ui-react.esm.js",
      "limit": "10 KB"
    }
  ]
}

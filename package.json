  "scripts": {
    "clean": "rimraf dist es lib",
    "start": "rollup -w",
    "build": "pnpm clean && rollup -c",
    "build-tsc": "pnpm tsc --project ./tsconfig-build.json",
    "test": "cross-env NODE_ENV=--experimental-vm-modules jest",
    "test:ci": "cross-env NODE_ENV=--experimental-vm-modules jest --ci --coverage --maxWorkers=2",
    "cypress:open": "cypress open",
    "lint": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky && pnpm build && rimraf stories/public/themes stories/public/skins stories/public/plugins stories/public/icons && cpy node_modules/tinymce/**/themes/** stories/public && cpy ./node_modules/tinymce/**/skins/** stories/public && cpy ./node_modules/tinymce/**/plugins/** stories/public && cpy ./node_modules/tinymce/**/icons/** stories/public",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "format": "./node_modules/.bin/prettier --write \"{src,stories,test}/**/*.{js,jsx,ts,tsx,json,css,less,scss,md}\"",
    "lint-staged": "lint-staged",
    "uploadTestFiles": "tsx scripts/uploadTestFiles.ts",
    "uploadTestFiles2": "node --experimental-vm-modules scripts2/uploadTestFiles.js"
  },
